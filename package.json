{
  "name": "meme-terminal",
  "displayName": "meme-terminal",
  "description": "Plays meme sounds on terminal success and error",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "publisher": "SanskarRai",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SanskarRai/meme-template"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "meme-terminal.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.2"
  },
  "dependencies": {
    "play-sound": "^1.1.6"
  }
}